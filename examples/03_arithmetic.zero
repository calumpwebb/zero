# Arithmetic operators in Zero

fn main() {
    a: int = 10
    b: int = 3

    print(a + b)    # 13
    print(a - b)    # 7
    print(a * b)    # 30
    print(a % b)    # 1

    # Precedence: * before +
    print(2 + 3 * 4)    # 14, not 20
    print((2 + 3) * 4)  # 20

    # Compound assignment
    a += 5
    print(a)  # 15

    # Negative numbers
    print(-5)       # -5
    print(10 + -3)  # 7
    print(--5)      # 5 (double negation)
}
