# Fibonacci calculator in Zero

fn fib(n: int): int {
    if (n <= 1) {
        return n
    }
    return fib(n - 1) + fib(n - 2)
}

fn main() {
    # Print first 10 Fibonacci numbers
    print(fib(0))   # 0
    print(fib(1))   # 1
    print(fib(2))   # 1
    print(fib(3))   # 2
    print(fib(4))   # 3
    print(fib(5))   # 5
    print(fib(6))   # 8
    print(fib(7))   # 13
    print(fib(8))   # 21
    print(fib(9))   # 34
}
